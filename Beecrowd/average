using System; 

class URI {

    static void Main(string[] args) { 

    double a, b, c, M;
            a = Convert.ToDouble(Console.ReadLine());
            b = Convert.ToDouble(Console.ReadLine());
            c = Convert.ToDouble(Console.ReadLine());
            M = ((a * 2 + b * 3 + c * 5) / (2 + 3 + 5));
            Console.WriteLine("MEDIA = " + M.ToString("f1"));

            Console.ReadKey();

    }

}
