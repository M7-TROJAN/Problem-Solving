using System; 

class URI {

    static void Main(string[] args) { 

    int A = readNumber(),
                B = readNumber(),
                SOMA = sumToNumbers(A, B); 

            Console.WriteLine("SOMA = {0}", SOMA);
    }
    
    static int readNumber() => int.Parse(Console.ReadLine());
    static int sumToNumbers(int number1, int number2) => number1 + number2;

}
