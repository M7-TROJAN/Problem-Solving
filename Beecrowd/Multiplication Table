using System; 

class URI {

static void Main(string[] args) { 
            printMultiplicationTable(readNumber());
    }
static int readNumber() => int.Parse(Console.ReadLine());
static int multiply(int i, int n) => i * n;
        static void printMultiplicationTable(int n)
        {
            for (int i = 1; i <= 10; i++)
                Console.WriteLine("{0} x {1} = {2}", i, n, multiply(i, n));
        }
}
