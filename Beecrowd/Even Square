using System; 

class URI {

    static void Main(string[] args) { 
printEvenSquare(readNumber());
    }
 static int readNumber() => int.Parse(Console.ReadLine());
        static bool isEven(int number) => number % 2 == 0;
        static int squareNumber(int number) => number * number;
        static void printEvenSquare(int n)
        {
            for (int i = 1; i <= n; i++)
                if (isEven(i))
                    Console.WriteLine("{0}^2 = {1}", i, squareNumber(i));
        }
}
