using System; 

class URI {

    static void Main(string[] args) { 
  printEvenNumbersInRange(1, 100);

    }
static bool isEvenNumber(int number) =>
            number % 2 == 0;
static void printEvenNumbersInRange(int firstNumber, int lastNumber)
        {
            for(int i=firstNumber;i<=lastNumber;i++)
                if(isEvenNumber(i))
                    Console.WriteLine(i);
        }
}
