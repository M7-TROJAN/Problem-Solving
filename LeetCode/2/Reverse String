public class Solution {
    public void ReverseString(char[] s) {
        char temp = ' ';
        int lastIndex = s.Length - 1;
        for (int i = 0; i < s.Length / 2; i++)
        {
            temp = s[i];
            s[i] = s[lastIndex];
            s[lastIndex] = temp;

            lastIndex--;
        } 
    }
}
