static int NumIdenticalPairs(int[] nums)
        {
            int counter = 0;
            for (int i = 0; i < nums.Length - 1; i++)
            {
                for (int j = i + 1; j < nums.Length; j++)
                {
                    if (nums[i] == nums[j] && i < j) counter++;
                }
            }
            return counter;
        }
