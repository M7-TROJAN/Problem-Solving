using System;
using System.Collections.Generic;

namespace lab
{
    class Program
    {
        static void Main()
        {
            Console.WriteLine(isLowerCase("Saleem"));
        }
        static char charToLowerCase(char c) =>
            (int)c >= 65 && (int)c <= 90 ? (char)((int)c + 32) : c;
        static bool isLowerCase(string s)
        {
            for (int i = 0; i < s.Length; i++)
                if (s[i] != charToLowerCase(s[i]))
                    return false;
            return true;
        }
    }
}
