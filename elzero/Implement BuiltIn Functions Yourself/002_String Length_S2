using System;
using System.Collections.Generic;
using System.IO;
using System.Linq;

namespace lab
{
    class Program
    {
        static void Main()
        {
            Console.WriteLine(length("saleem"));
        }
        static int length(string s)
        {
            int stringLength = 0;

            foreach (char c in s)
                stringLength++;

            return stringLength;
        }
    }
}
