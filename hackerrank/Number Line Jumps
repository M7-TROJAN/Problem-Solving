 public static string kangaroo(int x1, int v1, int x2, int v2)
    {
       string response = "NO";
       bool canCatchUp = (v2 < v1);
       if(canCatchUp)
       {
        bool willIntersectOnLand = (x1 - x2) % (v2 - v1) == 0;
        if(willIntersectOnLand)
        {
            response = "YES";
        }
        }
     return (response); 
    }
