public static List<int> compareTriplets(List<int> a, List<int> b)
    {
        int a1=0, b1 =0;
        List<int> result = new List<int>(2);
        
        for(int i=0; i<a.Count;i++){
            if(a[i] > b[i]) a1++;
            if(a[i] < b[i]) b1++;
        }
        result.Add(a1);
        result.Add(b1);
        
        return result;
        
    }
