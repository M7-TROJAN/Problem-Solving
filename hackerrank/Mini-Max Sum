public static void miniMaxSum(List<int> arr)
    {
       long max = arr.Max();
            long min = arr.Min();
            long minR = 0;
            long maxR = 0;

            foreach(int i in arr)
            {
                minR += i;
                maxR += i;
            }
            Console.WriteLine( (minR-max) + " " + (maxR-min));
    }
